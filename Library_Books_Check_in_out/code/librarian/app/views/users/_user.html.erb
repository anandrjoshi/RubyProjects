<%= raw "<tr id='user_#{user.id}'>" %>
  <DIV id="error_details"></DIV>
  <td><%= user.first_name %>
  <%= user.last_name %></td>
  <td><%= link_to 'Show', user %></td>
  <td><%= link_to 'Edit', edit_user_path(user) %></td>
  <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete, :remote => true %></td>
</tr>
