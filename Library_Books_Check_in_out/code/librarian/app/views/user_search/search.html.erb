<h1>Search Members</h1>
 
    <%= render 'user_search' %>
  
<% if @users != nil %>
<% if @users.count > 0 %>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
	<th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr class="<%= cycle("even", "odd") %>">
    <%= render :partial => 'user', :locals => {:user => user }%>
<% end %>
</table>
<% else %>
<p>No users were found.</p>
<% end %>
<% end %>


